/* 文章场景相关 */
.container {
  overflow: hidden;
}
.set_box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
}
/* 左侧时间 */
.set_time {
  width: 10%;
  position: relative;
}
.set_time view {
  position: absolute;
  margin-top: 38rpx;
  right: -20rpx;
  color: #999999;
  font-weight: 400;
  font-size: 25rpx;
}
/* 右侧内容 */
.set-1 {
  width: 90%;
}
.set-2 {
  margin-left: 100rpx;
  margin-top: 60rpx;
  padding-right: 10rpx;
  position: relative;
  background-color: #f4f4f4;
  padding: 20rpx;
  border-radius: 20rpx;
  color: #757575;
  font-size: 21rpx;
}
.set-2::after {
  content: '';
  /* 必须存在如果没有图标就留空 */
  top: -20rpx;
  /* 定位 距离*/
  border-left: #c6c6c6 1px solid;
  /* 横线颜色 */
  left: -44rpx;
  /* 定位 距离*/
  height: 120%;
  /* 高度 */
  position: absolute;
  /* 定位 */
}
.set-2::before {
  content: "\e64d";
  /* 必须存在如果没有图标就留空 */
  color: #069CF8;
  /* 设置颜色 */
  position: absolute;
  /* 定位 */
  left: -70rpx;
  top: -32rpx;
  /* 移动到左边 */
  font-weight: bold;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  font-family: "iconfont" !important;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  font-size: 54rpx;
  /* 图标大小 */
  font-style: normal;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  -webkit-font-smoothing: antialiased;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  -moz-osx-font-smoothing: grayscale;
  /* 图标样式在复制的图标文件中复制相对应的图标样式必须存在 */
  z-index: 1;
}
.info {
  width: 123rpx;
  height: 33rpx;
  background-color: #069CF8;
  border-radius: 5rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 21px;
  color: #fff;
  margin: 25rpx 0;
}
.lihe-hight {
  line-height: 40rpx;
}
